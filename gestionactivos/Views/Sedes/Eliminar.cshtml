@model SedesModel



<div class="card">
    <div class="card-header">
        Eliminar Sedes
    </div>
    <div class="card-body">

        <form asp-action="Eliminar" asp-controller="Sedes" method="post">

            <input asp-for="idSedes" type="hidden" class="form-control" />

            <div class="alert alert-danger" role="alert">
                Desea eliminar el Cliente: @Html.DisplayTextFor(m => m.NombreSede) ?
            </div>


            <button type="submit" class="btn btn-danger">Eliminar</button>
            <a asp-action="Listar" asp-controller="Sedes" class="btn btn-warning">Volver a la lista</a>
        </form>



    </div>
</div>